{
  "tasks": {
    "dev": "deno serve --watch --unstable-kv --env -A src/main.ts",
    "docker": "./docker-serve.sh",
    "test": "deno test tests/",
    "deploy": "deployctl deploy --project kmurp-link-shortener"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/http": "jsr:@std/http@1",
    "@preact": "npm:preact@10.28.2",
    "preact": "npm:preact@^10.28.2",
    "preact-render-to-string": "npm:preact-render-to-string@6.6.5",
    "@deno/kv-oauth": "https://jsr.io/@deno/kv-oauth/0.11.0/mod.ts",
    "@std/collections": "jsr:@std/collections@^1.1.4"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact"
  },
  "deploy": {
    "entrypoint": "src/main.ts"
  } 
}